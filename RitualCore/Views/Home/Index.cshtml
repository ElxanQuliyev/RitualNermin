        <!-- Slider -->
        <div class="col-lg-8">
                        <div id="myCarousel" class="carousel slide shadow">
                            <!-- main slider carousel items -->
                            <div class="carousel-inner">
                                @{ int count = 0;}
                                @foreach (var video in Model.Videos)
                                {
                                    <div class="@(count == 0 ? "active" : "") carousel-item" data-slide-number="@count">
                                        <a href="https://youtu.be/@video.YoutubeLink" target="_blank">

                                            <img src="Uploads/@video.Picture.Url" class="img-fluid">
                                        </a>
                                    </div>
                                    count++;
                                }
                            </div>
                            <!-- main slider carousel nav controls -->


                            <ul class="carousel-indicators list-inline mx-auto border px-1">
                                @{ int count2 = 0;}
                                @foreach (var video in Model.Videos)
                                {
                                    <li class="list-inline-item @(count2 == 0 ? "active" : "")">
                                        <a id="carousel-selector-@count2" class="selected" data-slide-to="@count2" data-target="#myCarousel">
                                            <img src="Uploads/@video.Picture.Url" class="img-fluid">
                                        </a>
                                    </li>
                                    count2++;
                                }

                            </ul>
